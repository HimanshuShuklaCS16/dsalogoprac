import java.util.Arrays;
class abc
{
    public static void main(String args[])
    {
        int arr1[]={2,4,6,8};
        int arr2[]={2,4,6,8};
        if(Arrays.equals(arr1,arr2))
            System.out.println("arrays are equal");
        else
            System.out.println("arrays are unequal");
    }
}
