import java.util.*;
import java.lang.*;
class abc{
    public static  void main(String args[])
    {
        int arr[]={-1,-1,6,1,9,3,2,-1,4,-1};
        int i,ele;
        for(i=0;i<arr.length;i++)
        {
            if(arr[i]>=0&&arr[i]!=i)
            {
                ele=arr[arr[i]];
                arr[arr[i]]=arr[i];
                arr[i]=ele;
            }
        }
       
          System.out.println(Arrays.toString(arr)); 

        
    }
}
    
